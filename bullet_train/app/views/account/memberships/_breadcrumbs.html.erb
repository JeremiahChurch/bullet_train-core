<% membership ||= @membership %>
<% team ||= @team || membership&.team %>
<%= render 'account/teams/breadcrumbs', team: team %>
<%= render 'shared/breadcrumb', label: t('.label'), url: [:account, team, :memberships] %>
<% if membership&.persisted? %>
  <%= render 'shared/breadcrumb', label: membership.label_string, url: [:account, membership] %>
<% end %>
<%= render 'shared/breadcrumbs/actions', only_for: 'memberships' %>
