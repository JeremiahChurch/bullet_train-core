<% endpoint ||= @endpoint %>
<% team ||= @parent || endpoint&.send(BulletTrain::OutgoingWebhooks.parent_association) %>
<%= render 'account/teams/breadcrumbs', team: team %>
<%= render 'shared/breadcrumb', label: t('.label'), url: [:account, team, :webhooks_outgoing_endpoints] %>
<% if endpoint&.persisted? %>
  <%= render 'shared/breadcrumb', label: endpoint.label_string, url: [:account, endpoint] %>
<% end %>
<%= render 'shared/breadcrumbs/actions', only_for: 'webhooks/outgoing/endpoints' %>
